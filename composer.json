{
  "name": "shiptoreps/pimpay-bundle",
  "description": "Pimpay API Bundle",
  "version": "0.0.1",
  "type": "library",
  "keywords": [
    "symfony",
    "bundle",
    "api",
    "pimpay"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Express Parcel Service"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "ext-zlib": "*",
    "ext-dom": "*",
    "ext-soap": "*",
    "symfony/dependency-injection": "^3.4",
    "symfony/config": "^3.4",
    "symfony/http-kernel": "^3.4"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0"
  },
  "autoload": {
    "psr-4": {
      "PimpayBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PimpayBundle\\": "tests/"
    }
  }
}
